{
  "name": "constantine",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm version minor && ng build --configuration production --build-optimizer --vendor-chunk --output-path docs --base-href .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:ci": "ng test --watch false",
    "test:noscourcemap": "ng run test:ci  --reporters spec --sourcemaps=false",
    "lint:fix": "tslint -p src/tsconfig.app.json --fix",
    "clean": "rm -rf ./docs/",
    "server": "cd docs && http-server",
    "prod": "npm run clean && npm run build:prod && npm run server",
    "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --browser ChromeTravisCi --reporters spec --environment prod && ng e2e && npm run build:travisci",
    "release": "npx standard-veprsion && git push --follow-tags origin master",
    "prettier": "prettier --single-quote --parser typescript --write \"./src/**/*.ts\" && prettier --single-quote --parser scss --write \"./src/**/*.scss\"",
    "prettier:ci": "prettier --single-quote --parser typescript --list-different \"./src/**/*.ts\" && prettier --single-quote --parser scss --list-different \"./src/**/*.scss\"",
    "analyze": "npm run clean && npm run build:prod && npx webpack-bundle-analyzer ./public/stats.json",
    "cloc": "cloc ./src",
    "git:count": "git rev-list --all --count"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~12.0.1",
    "@angular/google-maps": "^10.2.7",
    "@angular/localize": "^12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@popperjs/core": "^2.9.2",
    "@sendgrid/mail": "^7.4.4",
    "@types/chart.js": "^2.9.32",
    "angular-bootstrap-md": "^11.1.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "chart.js": "^3.2.1",
    "core-js": "^2.6.12",
    "firebase": "^8.6.2",
    "firebaseui": "^4.8.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "intl-tel-input": "^17.0.12",
    "jquery": "^3.6.0",
    "jquery.easing": "^1.4.1",
    "lodash": "^4.17.21",
    "ng2-tel-input": "^2.3.0",
    "ngrx-store-logger": "^0.2.4",
    "ngx-mask": "^2.7.3",
    "postcss": "^8.3.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "typescript-collections": "^1.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1200.1",
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/language-service": "~12.0.1",
    "@babel/core": "^7.14.3",
    "@ngrx/schematics": "^12.0.0",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^8.10.66",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.20.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.2.4",
    "standard-version": "^9.3.0",
    "cloc": "^2.8.0",
    "prettier": "^2.3.0",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:ci"
    }
  }
}
